{
  "name": "plutosols-angular2-server",
  "version": "1.0.0",
  "description": "Pluto Angular2.0 Development Server. A readiness server for Angular2.0 development.",
  "repository": {},
  "main": "app.js",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "npm run clean && tsc -w",

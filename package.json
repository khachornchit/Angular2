    "typings": "typings",
    "postinstall": "typings install",
    "clean": "rimraf app/js"
  },
  "author": "Pluto Solutions <contact@plutosols.com> (http://www.plutosols.com)",
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.13",
    "systemjs": "0.19.25",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "rimraf": "^2.6.2",
    "typescript": "^1.8.9",
    "typings": "^0.7.11"
  }
}
